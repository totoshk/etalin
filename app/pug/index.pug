    section#contacts.contacts
        -var namePattern="([A-Za-zА-Яа-я]{3,} ?){1,3}";
        -var phonePattern="[\+0-9]{7,}";
        -var emailPattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,4}";
        .container
            h2.section-title Контакты
            .section-stripe
            .contacts__info.clearfix
                h3.contacts__info-title Нас легко найти
                .contacts__info-item.contacts__info-adress
                    <i class="fa fa-home" aria-hidden="true"></i>
                    span Ул. Толе Би 304,
                    span г. Алматы, Казахстан
                .contacts__info-item.contacts__info-phone
                    <i class="fa fa-phone" aria-hidden="true"></i> 
                    span +7 727 356 11 05
                .contacts__info-item.contacts__info-callback
                    <i class="fa fa-envelope" aria-hidden="true"></i> 
                    span info@etalin.kz
            .contacts__map.clearfix
                <script type="text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3Aae59924baef1528d398083a2e12d3265c66f092fb46c9deb3aabceac7f6bd4b5&amp;width=100%&amp;height=100%&amp;lang=ru_UA&amp;scroll=true"></script>
            form(method="post").contacts__callback.clearfix#form-contacts
                p Если у вас остались вопросы, вы можете оставить сообщение и наши специалисты свяжутся с вами.
                input(type="text" name="name" placeholder="Имя" required pattern=namePattern).contacts__callback-field
                input(type="tel" name="phone" placeholder="Телефон" required pattern=phonePattern).contacts__callback-field
                input(type="email" name="email" placeholder="E-mail" required pattern=emailPattern).contacts__callback-field
                textarea(rows="10" cols="25" name="message" placeholder="Сообщение").contacts__callback-field
                input(type="submit" value="Отправить").btn.contacts__callback-btn
    .popup-outer
        .popup-form#popup-form
            h2.popup-form__title Заказать звонок
            <i class="fa fa-times popup-close" aria-hidden="true"></i>
            .section-stripe
            form(method="post").popup-form__callback.clearfix#form-callback
                input(type="text" name="name" placeholder="Имя" required pattern=namePattern).popup-form__field
                input(type="tel" name="phone" placeholder="Телефон" required pattern=phonePattern).popup-form__field
                input(type="submit" value="Подтвердить").btn.popup-form__btn
        .popup-form.popup-form__consult#popup-form__consult
            h2.popup-form__title Заказать консультацию
            <i class="fa fa-times popup-close" aria-hidden="true"></i>
            .section-stripe
            form(method="post").popup-form__callback.clearfix#form-consult
                input(type="text" name="name" placeholder="Имя" required pattern=namePattern).popup-form__field
                input(type="tel" name="phone" placeholder="Телефон" required pattern=phonePattern).popup-form__field
                textarea(rows="10" cols="5" name="message" placeholder="Сообщение").popup-form__field
                input(type="submit" value="Отправить").btn.popup-form__btn
        .popup-form.popup-form__consult#popup-form__service
            h2.popup-form__title Заказать расчет проекта
            <i class="fa fa-times popup-close" aria-hidden="true"></i>
            .section-stripe
            form(method="post").popup-form__callback.clearfix#form-consult
                input(type="text" name="name" placeholder="Имя и фамилия" required pattern=namePattern).popup-form__field
                input(type="text" name="company-name" placeholder="Название компании" required).popup-form__field
                input(type="tel" name="phone" placeholder="Телефон" required pattern=phonePattern).popup-form__field
                input(type="email" name="email" placeholder="Email" required pattern=emailPattern).popup-form__field
                textarea(rows="10" cols="5" name="message" placeholder="Введите данные о проекте").popup-form__field
                input(type="file" name="download" id="download-file").popup-form__fileloader
                label(for="download-file").popup-form__label Прикрепить файлы
                input(type="submit" value="Отправить").btn.popup-form__btn


